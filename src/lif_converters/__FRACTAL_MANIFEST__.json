{
  "manifest_version": "2",
  "task_list": [
    {
      "name": "Lif Plate Converter Task",
      "executable_non_parallel": "lif_plate_converter_init_task.py",
      "executable_parallel": "lif_converter_compute_task.py",
      "meta_non_parallel": {
        "cpus_per_task": 1,
        "mem": 4000
      },
      "meta_parallel": {
        "cpus_per_task": 1,
        "mem": 12000
      },
      "args_schema_non_parallel": {
        "additionalProperties": false,
        "properties": {
          "zarr_urls": {
            "items": {
              "type": "string"
            },
            "title": "Zarr Urls",
            "type": "array",
            "description": "List of zarr urls."
          },
          "zarr_dir": {
            "title": "Zarr Dir",
            "type": "string",
            "description": "Output path to save the OME-Zarr file."
          },
          "lif_files_path": {
            "title": "Lif Files Path",
            "type": "string",
            "description": "Input path to the LIF file, or a folder containing LIF files."
          },
          "num_levels": {
            "default": 5,
            "title": "Num Levels",
            "type": "integer",
            "description": "The number of resolution levels. Defaults to 5."
          },
          "coarsening_xy": {
            "default": 2,
            "title": "Coarsening Xy",
            "type": "integer",
            "description": "The scaling factor for the xy axes. Defaults to 2.0."
          },
          "overwrite": {
            "default": false,
            "title": "Overwrite",
            "type": "boolean",
            "description": "If True, the zarr store will be overwritten."
          }
        },
        "required": [
          "zarr_urls",
          "zarr_dir",
          "lif_files_path"
        ],
        "type": "object",
        "title": "LifPlateConverterInitTask"
      },
      "args_schema_parallel": {
        "$defs": {
          "ComputeInputModel": {
            "description": "Input model for the lif_converter_compute_task.",
            "properties": {
              "lif_path": {
                "title": "Lif Path",
                "type": "string"
              },
              "scene_name": {
                "title": "Scene Name",
                "type": "string"
              },
              "num_levels": {
                "default": 5,
                "minimum": 0,
                "title": "Num Levels",
                "type": "integer"
              },
              "coarsening_xy": {
                "default": 2,
                "minimum": 1,
                "title": "Coarsening Xy",
                "type": "integer"
              },
              "overwrite": {
                "default": false,
                "title": "Overwrite",
                "type": "boolean"
              },
              "plate_mode": {
                "default": true,
                "title": "Plate Mode",
                "type": "boolean"
              }
            },
            "required": [
              "lif_path",
              "scene_name"
            ],
            "title": "ComputeInputModel",
            "type": "object"
          }
        },
        "additionalProperties": false,
        "properties": {
          "zarr_url": {
            "title": "Zarr Url",
            "type": "string",
            "description": "The path to the zarr store."
          },
          "init_args": {
            "$ref": "#/$defs/ComputeInputModel",
            "title": "Init Args",
            "description": "The input parameters for the conversion."
          }
        },
        "required": [
          "zarr_url",
          "init_args"
        ],
        "type": "object",
        "title": "LifConverterComputeTask"
      },
      "docs_info": "## lif_plate_converter_init_task\nInitialize the conversion of LIF files to an OME-Zarr - Plate.\n## lif_converter_compute_task\nConvert a single acquisition (well) in inside an OME-Zarr plate.\n"
    },
